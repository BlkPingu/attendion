<h1><%= @dodyll.title %></h1>

<p>
  <% if @dodyll.description.present? %>
  <strong>Description: </strong> <%= @dodyll.description %><br />
  <% end %>
  <% if @dodyll.location.present? %>
    <strong>Location:</strong> <%= @dodyll.location %><br />
  <% end %>
  <table>
  <tr> <th></th>
  <% @dodyll.start.upto(@dodyll.end) do |date| %>
  <th> <%= date %> </th>
  <% end %>
  </tr>
    <%= form_with(model: [ @dodyll, @dodyll.votes.build ], local: true) do |f| %>
  <tr> <td>
    <%= f.label :user %><br>
    <%= f.text_field :user %>
    </td>
    <% @dodyll.start.upto(@dodyll.end) do |date| %>
      <td>
        <%= hidden_field_tag "vote" %>
        <%= check_box_tag "vote", true %></td>
    <% end %>
  </tr>
  </table>
      <p>
        <%= f.submit %>
      </p>
    <% end %>
</p>

  <%= link_to 'Back', dodylls_path %>