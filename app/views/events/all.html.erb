

<h1>All events</h1>





    <table width="100%">
      <tr>
        <th>Title</th>
        <th>Date</th>
        <th>Time</th>
        <th>Where</th>
        <th>Admin</th>
        <th></th>
      </tr>

      <% Event.all.each do |e| %>

      <tr>
        <td><%= e.title %></td>
        <td><%= e.date %></td>
        <td><%= e.time %></td>
        <td><%= e.location %></td>
        <td><%= e.user.name %></td>
        <td><%= submit_tag "join event" %></td>
      </tr>


      <% end %>
    </table>

